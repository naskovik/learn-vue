{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "trailing-comma": false,
        "space-before-function-paren": false,
        "quotemark": [true, "single"],
        "semicolon": false,
        "no-console": false
    },
    "rulesDirectory": []
}